#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/potree-upload/public/potree/node_modules/.pnpm/gulp-cli@2.3.0/node_modules/gulp-cli/bin/node_modules:/root/repos/potree-upload/public/potree/node_modules/.pnpm/gulp-cli@2.3.0/node_modules/gulp-cli/node_modules:/root/repos/potree-upload/public/potree/node_modules/.pnpm/gulp-cli@2.3.0/node_modules:/root/repos/potree-upload/public/potree/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/repos/potree-upload/public/potree/node_modules/.pnpm/gulp-cli@2.3.0/node_modules/gulp-cli/bin/node_modules:/root/repos/potree-upload/public/potree/node_modules/.pnpm/gulp-cli@2.3.0/node_modules/gulp-cli/node_modules:/root/repos/potree-upload/public/potree/node_modules/.pnpm/gulp-cli@2.3.0/node_modules:/root/repos/potree-upload/public/potree/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/gulp.js" "$@"
else
  exec node  "$basedir/../../bin/gulp.js" "$@"
fi
